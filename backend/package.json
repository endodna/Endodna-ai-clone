{
  "name": "biosai-backend",
  "version": "1.0.0",
  "description": "BiosAI AI Backend API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --ignore dist/ --ignore node_modules/ --ext ts --exec \"npm run compile\"",
    "compile": "tsc && node dist/server.js",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint --fix src/**/*.ts --ext ts,tsx  --max-warnings 30",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "docker:prod": "docker compose up",
    "docker:build": "docker compose build"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.490.0",
    "@aws-sdk/client-sqs": "^3.490.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@prisma/client": "^5.7.0",
    "@supabase/supabase-js": "^2.38.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "prisma": "^5.7.0",
    "redis": "^4.6.10",
    "typescript-eslint": "^8.46.2",
    "winston": "^3.11.0",
    "winston-cloudwatch": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.10.0",
    "@types/redis": "^4.0.11",
    "@types/winston": "^2.4.4",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "typescript": "^5.2.2"
  }
}
